<script>
function normalFontSize(){
	defaultFontSize =  12 + "px";
	$("body").css('font-size', defaultFontSize);
}

function normalContrast(){
	$(".wrapper").addClass("normal_mode");
	$(".wrapper").removeClass("negative");
	$(".wrapper").removeClass("black_white");
}
	function bwClick(){
		$(".wrapper").addClass("black_white");
		$(".wrapper").removeClass("negative");
		$(".wrapper").removeClass("normal_mode");
		$(".wrapper").removeClass("3333");
	}
	function contrastClick(){
		$(".wrapper").addClass("2222");
	}

	function inverseClick(){
		$(".wrapper").addClass("negative");
		$(".wrapper").removeClass("black_white");
		$(".wrapper").removeClass("normal_mode");
	}

// 	Font size enlarge
	function changeFontSize(target) {

		  var demo = document.getElementById("demo");
		  var computedStyle = window.getComputedStyle
		        ? getComputedStyle(demo) // Standards
		        : demo.currentStyle;     // Old IE

		  var fontSize;
		  var minSize = parseFloat("10");
		  var maxSize = parseFloat("22");

		  if (computedStyle) { // This will be true on nearly all browsers
		      fontSize = parseFloat(computedStyle && computedStyle.fontSize);

		      if (target == document.getElementById("button1")) {
			      if(fontSize < maxSize){
				  fontSize += 2;
			      }else{
				  fontSize = maxSize;
			      }
		        //fontSize += 2;
		      } else if (target == document.getElementById("button2")) {
			  if(fontSize > minSize){
				  fontSize -= 2;
			      }else{
				  fontSize = minSize;
			      }
		        //fontSize -= 2;
		      }
		      $("body").css('font-size', fontSize + "px");
		  }
		}

</script>


