<script src='https://www.google.com/recaptcha/api.js'></script>

<script>
	function recaptchaCallback(){
		 $('.captchaMsg').remove();
		 $('.button-row').html(" ");
		 $('.button-row').html("<input tabindex='2'	id='downloadRollPdf' value='Download' type='button' style='cursor: pointer;' onclick='downloadFile();'>");
	}

	function downloadFile(){
		if (grecaptcha.getResponse().length > 0) {
			window.location.replace($("#pdfFileUrlId").val());
		}else
			if (grecaptcha.getResponse().length == 0) {
				$('.button-row').html(" ");
				$('.button-row').html("<label class='captchaMsg' style='font-weight: bold;color: red;'>Please verify the 'Captcha' to download Electoral Roll !!!</label>");
			}
	}
</script>


